	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/timschwab/cheatsheet/issues"
	},
	"homepage": "https://github.com/timschwab/cheatsheet#readme",
	"devDependencies": {
		"electron": "^2.0.8",
		"husky": "^1.3.1",
		"lint-staged": "^8.1.3",
		"prettier": "^1.16.4"
	},
	"dependencies": {
		"bluebird": "^3.5.3",
		"bootstrap": "^4.3.1",
		"jquery": "^3.4.0",
		"marked": "^0.7.0",
		"popper.js": "^1.15.0",
		"redis": "^2.8.0",
		"vue": "^2.5.20"
	},
	"husky": {
